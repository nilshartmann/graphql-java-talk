{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "*.iml": true,
    "yarn-error.log": true,
    "node_modules": true,
    "yarn.lock": true,
    "querytypes": true
  },
  "eslint.packageManager": "yarn"
}
